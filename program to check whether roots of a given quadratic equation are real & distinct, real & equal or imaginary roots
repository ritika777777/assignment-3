int main()
{
    int a,b,c;
    printf("enter the value of a,b and  c");
    scanf("%d%d%d",&a,&b,&c);
    if(b*b-4*a*c>0){
       printf("distinct and real roots ");
       }
    else if(b*b-4*a*c==0){
        printf("equal and real roots");
        }
    else{
        printf("imaginary");
        }
    getch();
}
