#include<stdio.h>
#include<conio.h>
int main()
{
    int side1,side2,side3;
    printf("enter sides of a tringle");
    scanf("%d%d%d",&side1,&side2,&side3);
    if(side1+side2>side3 || side2+side3>side1 || side1+side3>side2){
        printf("it is a valid triangle");
    }

     else{
        printf("it is not a valid triangle");

    }
            return 0;
}

