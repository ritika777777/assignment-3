#include<stdio.h>
#include<ctype.h>
int main()
{

    char ch;
    printf("enter a character: ");
    scanf("%c",&ch);
    {
        if(isupper(ch))
            printf("uppercase character");
        else if(islower(ch))
            printf("lower case character");
        else
            printf("not a character");

    }
    return 0;
}

